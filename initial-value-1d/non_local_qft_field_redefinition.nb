Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"IPPHigherToSecondRep", "[", "k_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{
        SuperscriptBox["\[Phi]", 
         TagBox[
          RowBox[{"(", "k", ")"}],
          Derivative],
         MultilineFunction->None], "[", "t", "]"}], "n_."], "X_"}], 
     "\[RuleDelayed]", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"k", "-", "2"}]], 
      RowBox[{
       SuperscriptBox["\[Phi]", 
        TagBox[
         RowBox[{"(", "2", ")"}],
         Derivative],
        MultilineFunction->None], "[", "t", "]"}], 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"X", " ", 
         SuperscriptBox[
          RowBox[{
           SuperscriptBox["\[Phi]", 
            TagBox[
             RowBox[{"(", "k", ")"}],
             Derivative],
            MultilineFunction->None], "[", "t", "]"}], 
          RowBox[{"n", "-", "1"}]]}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", 
          RowBox[{"k", "-", "2"}]}], "}"}]}], "]"}]}]}], "}"}]}], 
  ";"}]], "Input",
