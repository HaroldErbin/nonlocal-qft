f0f61f32f64c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"findQuadRedef", "[", 
    RowBox[{"p_", ",", "expr_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dblcoll", ",", "factor", ",", "sol"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"dblcoll", "=", 
       RowBox[{
        RowBox[{"CoefficientList", "[", 
         RowBox[{
          RowBox[{"expr", "//", "Expand"}], ",", 
          RowBox[{
           SuperscriptBox["\[Phi]", "\[Prime]\[Prime]",
            MultilineFunction->None], "[", "t", "]"}]}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;", 
          RowBox[{"-", "1"}]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"factor", "=", 
       RowBox[{"dblcoll", ".", 
        RowBox[{"Table", "[", 
         RowBox[{
          SuperscriptBox[
           RowBox[{
            SuperscriptBox["\[Phi]", "\[Prime]\[Prime]",
             MultilineFunction->None], "[", "t", "]"}], "n"], ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "0", ",", 
            RowBox[{
             RowBox[{"Length", "[", "dblcoll", "]"}], "-", "1"}]}], "}"}]}], 
         "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"sol", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{"factor", "\[Equal]", "0"}], ",", 
           RowBox[{
            RowBox[{"\[Delta]\[Phi]", "[", 
             RowBox[{"2", 
              RowBox[{"(", 
               RowBox[{"p", "-", "1"}], ")"}]}], "]"}], "[", "t", "]"}]}], 
          "]"}], "//", "Simplify"}], "//", "Flatten"}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.829433451284417*^9, 3.829433500882537*^9}, {
   3.829433594984605*^9, 3.829433633355371*^9}, {3.8294336949991817`*^9, 
   3.829433702867914*^9}, 3.829433735455399*^9, 3.829867711724997*^9, {
