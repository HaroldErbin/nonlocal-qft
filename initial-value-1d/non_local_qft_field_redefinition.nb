Cell[TextData[{
 "Integrate by part ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[PartialD]", "t"], "\[Phi]"}], ")"}], "p"], 
