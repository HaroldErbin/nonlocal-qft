Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"reduceHigherTerms", "[", "expr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"orders", ",", "minOrder"}], "}"}], ",", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{"orders", "=", 
       RowBox[{
        RowBox[{"findOrders", "[", 
         RowBox[{"expr", "//", "Expand"}], "]"}], "//", "Flatten"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"minOrder", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"Select", "[", 
         RowBox[{"orders", ",", 
          RowBox[{
           RowBox[{"#", ">", "2"}], "&"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"minOrder", "\[Equal]", "\[Infinity]"}], ",", 
        "\[IndentingNewLine]", "expr", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"expr", "//", "Expand"}], ")"}], "/.", 
          RowBox[{"IPPHigherToSecondRep", "[", "minOrder", "]"}]}], "//", 
         "Simplify"}]}], "\[IndentingNewLine]", "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{
