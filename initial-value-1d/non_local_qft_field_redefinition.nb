3b0eba110c78"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"shiftRedef", "[", 
    RowBox[{"p_", ",", "expr_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rep", ",", "redef"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sol", "=", 
       RowBox[{"findQuadRedef", "[", 
        RowBox[{"p", ",", 
         RowBox[{"expr", "//", "Expand"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"redef", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"\[Delta]\[Phi]", "[", 
           RowBox[{"2", 
            RowBox[{"(", 
             RowBox[{"p", "-", "1"}], ")"}]}], "]"}], "[", "t", "]"}], "/.", 
         "sol"}], "//", "Simplify"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"rep", "=", 
       RowBox[{"sol", "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"\[Delta]_", "\[Rule]", "F_"}], "}"}], "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"\[Delta]", "\[Rule]", 
            RowBox[{"F", "+", "\[Delta]"}]}], "}"}]}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"expr", "//", "Expand"}], ")"}], "/.", "rep"}], "//", 
         "Simplify"}], ",", "redef"}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
