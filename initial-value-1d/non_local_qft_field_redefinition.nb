Cell[BoxData[
 RowBox[{
  RowBox[{"IPPLinToSecondRep", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"X_.", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[PartialD]", "t"], 
         RowBox[{"\[Phi]", "[", "t", "]"}]}], ")"}], "p_"]}], 
     "\[RuleDelayed]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "p"}], ")"}], 
      RowBox[{
       SuperscriptBox["\[Phi]", 
        TagBox[
         RowBox[{"(", "2", ")"}],
         Derivative],
        MultilineFunction->None], "[", "t", "]"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[PartialD]", "t"], 
         RowBox[{"\[Phi]", "[", "t", "]"}]}], ")"}], 
       RowBox[{"p", "-", "2"}]], 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"MatchQ", "[", 
         RowBox[{"X", ",", 
          RowBox[{"c_", " ", 
           SuperscriptBox[
            RowBox[{"\[Phi]", "[", "t", "]"}], "_."]}]}], "]"}], ",", 
        RowBox[{"X", "/.", 
         RowBox[{"{", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"\[Phi]", "[", "t", "]"}], "q_."], "\[RuleDelayed]", 
           FractionBox[
            SuperscriptBox[
             RowBox[{"\[Phi]", "[", "t", "]"}], 
             RowBox[{"q", "+", "1"}]], 
            RowBox[{"1", "+", "q"}]]}], "}"}]}], ",", 
        RowBox[{
         RowBox[{"\[Phi]", "[", "t", "]"}], " ", "X"}]}], "]"}]}]}], "}"}]}], 
  ";"}]], "Input",
