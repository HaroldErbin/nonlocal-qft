Cell["\<\
It seems depth is not moving from its original value -1/6 too! Now try to \
check it analytically. To do that we need to solve for extremum analytically, \
which we can do with the following perturbative ansatz and the position of \
critical point:\
\>", "Text",
 CellChangeTimes->{{3.8347802400189457`*^9, 3.834780328918886*^9}, {
  3.8353753040309563`*^9, 3.835375307199492*^9}, {3.8353753993971653`*^9, 
  3.835375402901238*^9}, {3.837204906561386*^9, 
  3.837204909683676*^9}},ExpressionUUID->"41af228d-1cb8-4290-b530-\
f78fa42e82cd"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "B", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Phi]A", "=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"B", "[", "i", "]"}], 
      RowBox[{"r", "^", "i"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0", ",", "8", ",", "2"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"coeff", "=", 
   RowBox[{"CoefficientList", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Series", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"Pot", "[", 
            RowBox[{"\[Phi]", ",", "r"}], "]"}], ",", "\[Phi]"}], "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{"\[Phi]", "\[Rule]", "\[Phi]A"}], "}"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"r", ",", "0", ",", "8"}], "}"}]}], "]"}], "//", "Normal"}], 
     ",", "r"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "0"}], ",", 
   RowBox[{"i", "\[LessEqual]", "8"}], ",", 
   RowBox[{"i", "=", 
    RowBox[{"i", "+", "2"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Set", "@@@", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"coeff", "[", 
